{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "yachtdaywork.com" }],
      "destination": "https://www.yachtdaywork.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/e/:id",
      "destination": "/api/event-og?event=:id"
    }
  ]
}