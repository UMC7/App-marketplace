{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "yachtdaywork.com" }
      ],
      "destination": "https://www.yachtdaywork.com/:1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.yachtdaywork.com" },
        { "type": "protocol", "value": "http" }
      ],
      "destination": "https://www.yachtdaywork.com/:1",
      "permanent": true
    }
  ]
}